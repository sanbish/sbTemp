@{
    ViewBag.Title = "Dashboard";
}
<div class="row">
    <div class="col-md-4">
        <div class="card card-chart">
            <div class="card-header card-header-success">
                <div class="ct-chart" id="dailySalesChart"></div>
            </div>
            <div class="card-body">
                <h4 class="card-title">Daily Stocks</h4>
                <p class="card-category">
                    <span class="text-success"><i class="fa fa-long-arrow-up"></i> 55% </span> increase in today stocks.
                </p>
            </div>
            <div class="card-footer">
                <div class="stats">
                    <i class="material-icons">access_time</i> updated 4 minutes ago
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="card card-chart">
            <div class="card-header card-header-warning">
                <div class="ct-chart" id="websiteViewsChart"></div>
            </div>
            <div class="card-body">
                <h4 class="card-title">Monthly Stock</h4>
                <p class="card-category">
                    <span class="text-success"><i class="fa fa-long-arrow-up"></i> 55% </span> increase in today stocks.
                </p>
            </div>
            <div class="card-footer">
                <div class="stats">
                    <i class="material-icons">access_time</i> updated 4 minutes ago
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="card card-chart">
            <div class="card-header card-header-danger">
                <div class="ct-chart" id="completedTasksChart"></div>
            </div>
            <div class="card-body">
                <h4 class="card-title">Out of Stocks</h4>
                <p class="card-category">
                    <span class="text-success"><i class="fa fa-long-arrow-up"></i> 55% </span> increase in today stocks.
                </p>
            </div>
            <div class="card-footer">
                <div class="stats">
                    <i class="material-icons">access_time</i> updated 4 minutes ago
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-5">
        <div class="card">
            <div class="card-header card-header-warning">
                <h4 class="card-title">Products going to expire soon</h4>
                <p class="card-category">As on @DateTime.Now.ToString("dd, mmm yyyy")</p>
            </div>
            <div class="card-body table-responsive">
                <table class="table table-hover">
                    <thead class="text-warning">
                    <th>ID</th>
                    <th>Name</th>
                    <th>Expiry Date</th>
                    <th>Warranty Till</th>
                    </thead>
                    <tbody>
                        <tr>
                            <td>1</td>
                            <td>6 Watt LED Bulb</td>
                            <td>25/12/2018</td>
                            <td>25/12/2018</td>
                        </tr>
                        <tr>
                            <td>2</td>
                            <td>8 Watt LED Tubelight</td>
                            <td>12/11/2018</td>
                            <td>12/11/2018</td>
                        </tr>
                        <tr>
                            <td>3</td>
                            <td>8 Watt LED Bulb</td>
                            <td>12/12/2018</td>
                            <td>12/12/2018</td>
                        </tr>
                        <tr>
                            <td>4</td>
                            <td>18 Watt LED Tubelight</td>
                            <td>12/11/2018</td>
                            <td>12/11/2018</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="col-md-7">
        @{Html.RenderAction("_Index", "Notifications");}
    </div>
</div>